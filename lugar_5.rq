PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?x 
WHERE {
{
   ?x rdfs:label ?label .
   FILTER regex(str(?label), "accesible", "i")
}UNION{
   ?x rdfs:label ?label .
   FILTER regex(str(?label), "tipo de accesibilidad", "i")

}
}

